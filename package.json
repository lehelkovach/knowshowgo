{
  "name": "knowshowgo",
  "version": "0.1.0",
  "description": "A fuzzy ontology knowledge graph for semantic memory",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src/**/*.js",
    "docs": "jsdoc -c jsdoc.json",
    "build": "echo 'No build step required for ES modules'",
    "start": "node src/server/rest-api.js",
    "dev": "node --watch src/server/rest-api.js"
  },
  "keywords": [
    "knowledge-graph",
    "ontology",
    "semantic-memory",
    "embeddings",
    "fuzzy-matching",
    "ai",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "arangojs": "^8.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jsdoc": "^4.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lehelkovach/knowshowgo.git"
  }
}


